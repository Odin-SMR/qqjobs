{
  "linters": {
    "python": {
      "type": "pep8",
      "include": "(\\.py$)",
      "severity": {
        "W503": "disabled"
      }
    },
    "xml": {
      "type": "xml",
      "include": "(\\.xml$)"
    },
    "jshint": {
      "type": "jshint",
      "include": "(\\.js$)",
      "exclude": "(\\.min.js$)"
    }
  }
}
